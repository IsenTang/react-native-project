{
    "env": {
        "node": true, 
        "browser": true, 
        "es6": true 
    },
    "parser": "babel-eslint", 
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
        "eslint react/prop-types": 0,
        "react/forbid-prop-types": ["enabled", { "forbid": ["string"] }]
    }
}